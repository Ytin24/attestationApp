<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
			 x:Class="attestationApp.Views.CompletionView"
			 
             d:DesignWidth="800" d:DesignHeight="600">
	<StackPanel Margin="20" HorizontalAlignment="Center" VerticalAlignment="Center">
		<TextBlock Text="Спасибо за ответ!" FontSize="24" Margin="0,0,0,20"/>
		<StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Orientation="Horizontal" Spacing="10">
			<Button IsVisible="False" Content="Close" Command="{Binding CloseCommand}" Margin="0,0,0,10"/>
			<Button Content="Начать заново" Command="{Binding RestartCommand}"/>
		</StackPanel>

	</StackPanel>
</UserControl>
